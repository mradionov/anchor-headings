{
  "manifest_version": 2,
  "name": "Anchor headings",
  "description": "Adds anchors to headings",
  "version": "0.1.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/content.css"
      ]
    }
  ]
}
